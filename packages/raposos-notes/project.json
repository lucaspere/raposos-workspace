  "sourceRoot": "packages/raposos-notes/src",
  "targets": {
    "build": {
      "executor": "@wanews/nx-vite:build",
      "options": {}
    },
    "serve": {
      "executor": "@wanews/nx-vite:serve",
      "options": {}
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/raposos-notes/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx vitest --run",
        "cwd": "packages/raposos-notes"
      }
    }
  },
  "tags": []
}
